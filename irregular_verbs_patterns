                               'Nós/We': 'damos', 'Else/Elas/vocês/They': 'dão'}

    DIZER_PRESENTE_INDICATIVO = {'Eu/I': 'digo', 'Tu/You': 'dizes', 'Ele/Ela/você/He/She/It': 'diz',
                                'Nós/We': 'dizemos', 'Else/Elas/vocês/They': 'dizem'}


    TRAZER_PRESENTE_INDICATIVO = {'Eu/I': 'digo', 'Tu/You': 'dizes', 'Ele/Ela/você/He/She/It': 'diz',
                                'Nós/We': 'dizemos', 'Else/Elas/vocês/They': 'dizem'}


    # FIXED CONJUGATIONS FOR CONDICIONAL SIMPLES TENSE
    CONDICIONAL_TRAZER = {'Eu/I': 'traria', 'Tu/You': 'trarias', 'Ele/Ela/você/He/She/It': 'traria',
